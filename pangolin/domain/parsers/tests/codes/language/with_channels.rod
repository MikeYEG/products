    rules: "./my/rules.json".
    logic: "./my/logic.json".
    in: MyInputVar.
    out: MyOutputVar.
    matches:
        mySecond: MyTokenVariable, myEnter|myExit.
        myThird: MyTokenVariable, myEnter.
